{
    "manifest_version": 2,
    "name": "Form Filler",
    "description": "This extension fills in form inputs at a click of a button.",
    "version": "0.0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Click here now now!"
    },
    "permissions": [
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'"
}